{
  "name": "inventory",
  "version": "1.0.7",
  "description": "Electronics Components inventory",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron .",
    "clean": "rm -rf ./dist",
    "clean:win": "IF EXIST dist rmdir /s /q dist",
    "pack:win": "electron-packager . inventory --overwrite --out=dist --appname=inventory --app-copyright=\"(c) 2022 Muchirijohn\" --platform=win32 --arch=x64 --app-version=1.0.7 --icon=app/res/data/icon.png --ignore=versions --ignore=releases --ignore=docs --win32metadata.CompanyName=\"muchirijohn\" --win32metadata.ProductName=\"inventory\" --win32metadata.FileDescription=\"inventory\" --win32metadata.OriginalFilename=\"inventory.exe\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muchirijohn/inventory.git"
  },
  "keywords": [
    "inventory",
    "electronics",
    "parts"
  ],
  "author": "muchirijohn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muchirijohn/inventory/issues"
  },
  "homepage": "https://github.com/muchirijohn/inventory#readme",
  "devDependencies": {
    "electron": "^18.0.4",
    "electron-builder": "^23.0.3",
    "electron-packager": "^15.5.0",
    "electron-rebuild": "^3.2.7"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "builder-util-runtime": "^9.0.1",
    "chance": "^1.1.8",
    "fs-extra": "^10.1.0",
    "i": "^0.3.7",
    "jquery": "^3.6.0",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.3",
    "npm": "^8.7.0",
    "os-homedir": "^2.0.0",
    "qrcode": "^1.5.0",
    "sqlite3": "^5.0.4",
    "sweetalert": "^2.1.2"
  }
}
